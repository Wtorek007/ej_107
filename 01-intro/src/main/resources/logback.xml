<?xml version="1.0" encoding="UTF-8" ?>
<configuration>

    <logger name="org" level="INFO"/>

    <root level="ALL">
        <appender class="ch.qos.logback.core.ConsoleAppender">
            <encoder>
                <pattern>%d{HH:mm:ss.SSS} | %-5level | %thread | %m%n%rEx</pattern>
            </encoder>
        </appender>
    </root>
</configuration>
